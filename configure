#!/bin/bash

chmod +x buildlex
chmod +x buildyacc
chmod +x buildlib
chmod +x buildexec
chmod +x buildall
chmod +x buildtests
chmod +x testall
chmod +x run
chmod +x debug

mkdir logs
mkdir logs/tests
mkdir bin

cd bin

mkdir obj
mkdir builds
mkdir gensrc

cd builds

mkdir libmonga
mkdir monga

cd libmonga

mkdir include
mkdir debug
mkdir release

cd ../monga

mkdir debug
mkdir release

cd ../../../

./buildexec
./buildtests